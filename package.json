{
  "name": "nuxt-life",
  "version": "1.0.0",
  "description": "Game of Life",
  "author": "Jim Bacon",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "cypress open",
    "jest": "jest"
  },
  "dependencies": {
    "14": "^3.1.6",
    "@nuxtjs/axios": "^5.0.0",
    "cross-env": "^5.2.0",
    "node-sass": "^4.11.0",
    "nuxt": "^2.3.4",
    "sass-loader": "^7.1.0",
    "vue-loader": "^15.4.2",
    "vue-material": "^1.0.0-beta-10.2",
    "vue-router": "3.0.1"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.0.2",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.6.0",
    "cypress": "^3.1.3",
    "cypress-vue-unit-test": "^1.11.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^23.6.0",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "vue-jest": "^3.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^(@|~)/(.*)$": "<rootDir>/$2",
      "^(@@|~~)/(.*)$": "<rootDir>/$2"
    }
  }
}
